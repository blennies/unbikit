{
  "name": "unbikit",
  "version": "0.8.0",
  "description": "Decoder for `.bik` video files.",
  "keywords": [
    "audio",
    "bik",
    "bink",
    "codec",
    "decoder",
    "decompress",
    "games",
    "media",
    "player",
    "video"
  ],
  "author": "Blennies <blennies.dev@gmail.com>",
  "license": "(MIT OR Apache-2.0)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blennies/unbikit.git"
  },
  "homepage": "https://blennies.github.io/unbikit",
  "bugs": {
    "url": "https://github.com/blennies/unbikit/issues"
  },
  "type": "module",
  "sideEffects": false,
  "module": "./dist/unbikit.js",
  "engines": {
    "node": ">=22.18"
  },
  "packageManager": "pnpm@10.23.0+sha512.21c4e5698002ade97e4efe8b8b4a89a8de3c85a37919f957e7a0f30f38fbc5bbdd05980ffe29179b2fb6e6e691242e098d945d1601772cad0fef5fb6411e2a4b",
  "scripts": {
    "dev": "astro dev",
    "dev:app": "astro dev",
    "build": "tsdown",
    "build:app": "astro build",
    "check": "astro check && biome check *.ts *.mts *.json *.jsonc app src/bik-decoder.ts && prettier -c . && tsc && vitest run --testTimeout 10000",
    "ci:check": "astro check && biome ci *.ts *.mts *.json *.jsonc app src/bik-decoder.ts && prettier -c . && tsc",
    "preview": "astro preview",
    "preview:app": "astro preview",
    "test": "vitest --ui --testTimeout 10000",
    "test:coverage": "vitest --ui --coverage --testTimeout 20000",
    "test:cli": "vitest --testTimeout 10000",
    "ci:test": "vitest run --coverage --testTimeout 30000",
    "bench": "vitest bench --run"
  },
  "devDependencies": {
    "@arethetypeswrong/core": "0.18.2",
    "@astrojs/check": "0.9.5",
    "@astrojs/mdx": "4.3.12",
    "@astrojs/starlight": "0.36.2",
    "@biomejs/biome": "2.3.7",
    "@commitlint/config-conventional": "20.0.0",
    "@commitlint/types": "20.0.0",
    "@fontsource-variable/sora": "5.2.8",
    "@types/audioworklet": "0.0.91",
    "@types/node": "24.10.1",
    "@types/pngjs": "6.0.5",
    "@vitest/coverage-v8": "4.0.13",
    "@vitest/ui": "4.0.13",
    "astro": "5.16.0",
    "commitlint": "20.1.0",
    "globals": "16.5.0",
    "pngjs": "7.0.0",
    "prettier": "3.6.2",
    "publint": "0.3.15",
    "rolldown": "1.0.0-beta.51",
    "rollup-plugin-esbuild": "6.2.1",
    "sharp": "^0.34.5",
    "starlight-package-managers": "0.11.1",
    "starlight-theme-rapide": "0.5.2",
    "starlight-typedoc": "0.21.4",
    "tinybench": "5.1.0",
    "ts-extras": "0.16.0",
    "tsdown": "0.16.6",
    "type-fest": "5.2.0",
    "typedoc": "0.28.14",
    "typedoc-plugin-markdown": "4.9.0",
    "typescript": "5.9.3",
    "unplugin-unused": "0.5.6",
    "vite": "npm:rolldown-vite@7.2.7",
    "vitest": "4.0.13"
  },
  "pnpm": {
    "overrides": {
      "tinybench": "$tinybench",
      "vitest>vite": "$vite"
    },
    "patchedDependencies": {
      "starlight-theme-rapide": "patches/starlight-theme-rapide.patch"
    }
  },
  "exports": {
    ".": {
      "types": "./dist/unbikit.d.ts",
      "development": "./src/bik-decoder.ts",
      "default": "./dist/unbikit.js"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/unbikit.d.ts",
        "default": "./dist/unbikit.js"
      },
      "./package.json": "./package.json"
    }
  },
  "files": [
    "dist/**/*.d.cts",
    "dist/**/*.d.ts",
    "dist/**/*.cjs",
    "dist/**/*.js"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
