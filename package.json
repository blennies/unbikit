{
  "name": "unbikit",
  "version": "0.3.0",
  "description": "Decoder for `.bik` video files.",
  "keywords": [
    "audio",
    "bik",
    "bink",
    "codec",
    "decoder",
    "decompress",
    "media",
    "player",
    "video"
  ],
  "author": "Blennies <blennies.dev@gmail.com>",
  "license": "(MIT OR Apache-2.0)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blennies/unbikit.git"
  },
  "type": "module",
  "module": "./dist/unbikit.js",
  "main": "./dist/unbikit.cjs",
  "engines": {
    "node": ">=22.18"
  },
  "packageManager": "pnpm@10.22.0+sha512.bf049efe995b28f527fd2b41ae0474ce29186f7edcb3bf545087bd61fbbebb2bf75362d1307fda09c2d288e1e499787ac12d4fcb617a974718a6051f2eee741c",
  "scripts": {
    "dev": "vite",
    "build": "tsdown",
    "build:docs": "typedoc",
    "check": "biome check && prettier -c . && tsc && vitest run",
    "ci:check": "biome ci && prettier -c . && tsc",
    "preview": "vite preview",
    "preview:docs": "vite ./docs --port 6173",
    "test": "vitest --ui",
    "test:coverage": "vitest --ui --coverage --testTimeout 20000",
    "test:cli": "vitest",
    "ci:test": "vitest run --coverage --testTimeout 20000"
  },
  "devDependencies": {
    "@arethetypeswrong/core": "0.18.2",
    "@biomejs/biome": "2.3.5",
    "@commitlint/config-conventional": "20.0.0",
    "@commitlint/types": "20.0.0",
    "@mitata/counters": "0.0.8",
    "@types/audioworklet": "0.0.91",
    "@types/node": "24.10.1",
    "@types/pngjs": "6.0.5",
    "@vitest/coverage-v8": "^4.0.8",
    "@vitest/ui": "4.0.8",
    "commitlint": "20.1.0",
    "globals": "16.5.0",
    "mitata": "1.0.34",
    "pngjs": "7.0.0",
    "prettier": "3.6.2",
    "publint": "0.3.15",
    "rolldown": "1.0.0-beta.50",
    "rollup-plugin-esbuild": "6.2.1",
    "ts-extras": "0.16.0",
    "tsdown": "0.16.4",
    "type-fest": "5.2.0",
    "typedoc": "0.28.14",
    "typedoc-theme-fresh": "0.2.1",
    "typescript": "5.9.3",
    "unplugin-unused": "0.5.6",
    "vite": "npm:rolldown-vite@7.2.5",
    "vitest": "4.0.8"
  },
  "overrides": {
    "rolldown": "$rolldown",
    "vite": "$vite"
  },
  "exports": {
    ".": {
      "development": "./src/bik-decoder.ts",
      "import": {
        "types": "./dist/unbikit.d.ts",
        "default": "./dist/unbikit.js"
      },
      "require": {
        "types": "./dist/unbikit.d.cts",
        "default": "./dist/unbikit.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "import": {
          "types": "./dist/unbikit.d.ts",
          "default": "./dist/unbikit.js"
        },
        "require": {
          "types": "./dist/unbikit.d.cts",
          "default": "./dist/unbikit.cjs"
        }
      },
      "./package.json": "./package.json"
    }
  },
  "files": [
    "dist/**/*.d.cts",
    "dist/**/*.d.ts",
    "dist/**/*.cjs",
    "dist/**/*.cjs.map",
    "dist/**/*.js",
    "dist/**/*.js.map"
  ],
  "typedocOptions": {
    "entryPoints": [
      "./src/bik-decoder.ts"
    ],
    "includeVersion": true,
    "name": "unbikit",
    "plugin": [
      "typedoc-theme-fresh"
    ],
    "readme": ".github/README.md",
    "theme": "fresh"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
