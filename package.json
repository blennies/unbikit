{
  "name": "unbikit",
  "version": "0.10.2",
  "description": "Decoder for .bik (Bink) video files.",
  "keywords": [
    "audio",
    "bik",
    "bink",
    "codec",
    "decoder",
    "decompress",
    "games",
    "media",
    "player",
    "video"
  ],
  "author": "Blennies <blennies.dev@gmail.com>",
  "license": "(MIT OR Apache-2.0)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blennies/unbikit.git"
  },
  "homepage": "https://blennies.github.io/unbikit",
  "bugs": {
    "url": "https://github.com/blennies/unbikit/issues"
  },
  "type": "module",
  "module": "./dist/unbikit.js",
  "types": "./dist/unbikit.d.ts",
  "sideEffects": false,
  "engines": {
    "node": ">=22.18"
  },
  "packageManager": "pnpm@10.29.1+sha512.48dae233635a645768a3028d19545cacc1688639eeb1f3734e42d6d6b971afbf22aa1ac9af52a173d9c3a20c15857cfa400f19994d79a2f626fcc73fccda9bbc",
  "scripts": {
    "dev": "astro dev",
    "dev:app": "astro dev",
    "build": "tsdown",
    "build:app": "astro build",
    "check": "astro check && biome check *.ts *.mts *.json *.jsonc app src/bik-decoder.ts && prettier -c . && tsc && vitest run --testTimeout 10000",
    "ci:check": "astro check && biome ci *.ts *.mts *.json *.jsonc app src/bik-decoder.ts && prettier -c . && tsc",
    "preview": "astro preview",
    "preview:app": "astro preview",
    "test": "vitest --ui --testTimeout 10000",
    "test:coverage": "vitest --ui --coverage --testTimeout 20000",
    "test:cli": "vitest --testTimeout 10000",
    "ci:test": "vitest run --coverage --testTimeout 30000",
    "bench": "vitest bench --run"
  },
  "devDependencies": {
    "@arethetypeswrong/core": "0.18.2",
    "@astrojs/check": "0.9.6",
    "@astrojs/mdx": "4.3.13",
    "@astrojs/starlight": "0.37.6",
    "@biomejs/biome": "2.3.14",
    "@commitlint/config-conventional": "20.4.1",
    "@commitlint/types": "20.4.0",
    "@fontsource-variable/sora": "5.2.8",
    "@types/audioworklet": "0.0.94",
    "@types/node": "25.2.2",
    "@types/pngjs": "6.0.5",
    "@vitest/coverage-v8": "4.0.18",
    "@vitest/ui": "4.0.18",
    "astro": "5.17.1",
    "commitlint": "20.4.1",
    "globals": "17.3.0",
    "pngjs": "7.0.0",
    "prettier": "3.8.1",
    "publint": "0.3.17",
    "rollup-plugin-esbuild": "6.2.1",
    "sharp": "0.34.5",
    "starlight-package-managers": "0.12.0",
    "starlight-theme-rapide": "0.5.2",
    "starlight-typedoc": "0.21.5",
    "ts-extras": "0.17.0",
    "tsdown": "0.20.3",
    "type-fest": "5.4.4",
    "typedoc": "0.28.16",
    "typedoc-plugin-markdown": "4.10.0",
    "typescript": "5.9.3",
    "unplugin-unused": "0.5.7",
    "vite": "8.0.0-beta.13",
    "vitest": "4.0.18"
  },
  "pnpm": {
    "allowBuilds": {
      "esbuild": false,
      "sharp": false
    },
    "overrides": {
      "esbuild": "0.27.3",
      "lodash": "4.17.23",
      "tinybench": "https://registry.npmjs.org/tinybench/-/tinybench-5.1.0.tgz",
      "typescript": "$typescript",
      "vitest>vite": "$vite",
      "@vitest/mocker>vite": "$vite"
    },
    "patchedDependencies": {
      "starlight-theme-rapide": "patches/starlight-theme-rapide.patch"
    },
    "peerDependencyRules": {
      "allowedVersions": {
        "vite": "8.x"
      }
    }
  },
  "exports": {
    ".": {
      "types": "./dist/unbikit.d.ts",
      "development": "./src/bik-decoder.ts",
      "default": "./dist/unbikit.js"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/unbikit.d.ts",
        "default": "./dist/unbikit.js"
      },
      "./package.json": "./package.json"
    }
  },
  "files": [
    "dist/**/*.d.cts",
    "dist/**/*.d.ts",
    "dist/**/*.cjs",
    "dist/**/*.js"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
